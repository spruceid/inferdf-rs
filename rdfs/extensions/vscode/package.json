{
    "name": "rdfs",
    "displayName": "RDFs",
    "description": "RDF semantics definition language for Visual Studio Code",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.64.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "activationEvents": [
        "onLanguage:rdfs"
    ],
    "main": "./main.js",
    "contributes": {
        "languages": [{
            "id": "rdfs",
            "aliases": ["RDFs"],
            "extensions": [".rdfs"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rdfs",
            "scopeName": "source.rdfs",
            "path": "./grammar.json"
        }],
        "snippets": [
            {
                "language": "rdfs",
                "path": "./snippets.json"
            }
        ]
    }
}