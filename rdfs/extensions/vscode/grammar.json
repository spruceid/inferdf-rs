{
	"scopeName": "source.rdfs",
	"patterns": [
		{
			"name": "comment.line.documentation.rdfs",
			"begin": "##",
			"end": "\n"
		},
		{
			"name": "comment.line.number-sign.rdfs",
			"begin": "#",
			"end": "\n"
		},
		{
			"name": "keyword.rdfs",
			"match": "\\b(base|prefix|group|rule|forall|exists|where)\\b"
		},
		{
			"name": "constant.numeric.rdfs",
			"match": "\\b[0-9]+\\b"
		},
		{
			"name": "string.quoted.other.uri.rdfs",
			"begin": "<",
			"end": ">"
		},
		{
			"name": "variable.prefix.rdfs",
			"match": "\\b\\w+(?=:\\w)"
		},
		{
			"name": "variable.other.rdfs",
			"match": "\\?\\w+"
		}
	]
}