(?c rdf:type owl:FunctionalProperty) =>
	assert ((?c rdf:type owl:ObjectProperty) | (?c rdf:type owl:DatatypeProperty)) &
	((?x ?c ?y1) & (?x ?c ?y?) => assert ?x = ?y)

(?c rdf:type owl:InverseFunctionalProperty) =>
	assert (?c rdf:type owl:ObjectProperty) &
	((?x ?c ?y1) & (?x ?c ?y?) => assert ?x = ?y)

(?c rdf:type owl:SymmetricProperty) =>
	assert (?c rdf:type owl:ObjectProperty) &
	((?x ?c ?y) => (?y ?c ?x))

(?c rdf:type owl:TransitiveProperty) =>
	assert (?c rdf:type owl:ObjectProperty) &
	((?x ?c ?y) => (?y ?c ?z) => (?x ?c ?z))

(?x owl:equivalentClass ?y) =>
	(?a rdf:type ?x) => (?a rdf:type ?y)

(?x owl:disjointWith ?y) =>
	(?a rdf:type ?x) => !(?a rdf:type ?y))

(?x owl:equivalentProperty ?y) =>
	(?u ?x ?v) => (?u ?y ?v)

(?x owl:inverseOf ?y) =>
	(?u ?x ?v) => (?v ?y ?u)

(?x owl:sameAs ?y) =>
	?x = ?y

(?x owl:differentFrom ?y) =>
	?x != ?y

(?x owl:complementOf ?y) => 
	(?v rdf:type ?x) <=> !(?v rdf:type ?y)

(?x owl:unionOf ?list) => 
	(?list #rdf:listMember ?y) => (?v rdf:type ?y) => (?v rdf:type ?y)

(?x owl:intersectionOf ?list) => 
	(?v #owl:inIntersectionOf ?list) => (?v rdf:type ?x)

(?list rdf:first ?c) & (?list rdf:rest ?rest) =>
	(?v rdf:type ?c) & (?v #owl:inIntersectionOf ?rest) => (?v #owl:inIntersectionOf ?list)

(?x owl:oneOf ?list) => 
	(?list #rdf:listMember ?v) => (?v rdf:type ?y)

# Axioms
(owl:FunctionalProperty rdf:type owl:Class)
(owl:InverseFunctionalProperty rdf:type owl:Class)
(owl:SymmetricProperty rdf:type owl:Class)
(owl:TransitiveProperty rdf:type owl:Class)
(owl:DeprecatedClass rdf:type owl:Class)
